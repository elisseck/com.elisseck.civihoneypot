<?xml version="1.0"?>
<extension key="com.elisseck.civihoneypot" type="module">
  <file>civihoneypot</file>
  <name>civihoneypot</name>
  <description>A basic honeypot for CiviCRM forms</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Eli Lisseck</author>
    <email>lisseck@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/elisseck/com.elisseck.civihoneypot</url>
    <url desc="Documentation">https://github.com/elisseck/com.elisseck.civihoneypot</url>
    <url desc="Support">https://github.com/elisseck/com.elisseck.civihoneypot</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-09-16</releaseDate>
  <version>1.1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Civihoneypot</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Civihoneypot_Upgrader</upgrader>
</extension>
